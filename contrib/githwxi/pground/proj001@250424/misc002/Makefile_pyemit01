########################################################################
########################################################################
# Makefile for misc001
########################################################################
########################################################################
CAT=cat
BUN=bun
ECHO=echo
NODE=node
PRINTF=printf
PYTHON=python3
########################################################################
########################################################################
all:: pstrm000
########################################################################
clean:: ; rm -f *~
cleanall:: ; rm -f *~
cleanall:: ; rm -f OUTS/*~
cleanall:: ; rm -f OUTS/*_out.py
########################################################################
testall:: pstrm000; $(PYTHON) OUTS/$<_out.py
########################################################################
########################################################################
XATSHOME=./../../../../../../XATSHOME
########################################################################
#
XATS2PYD=\
$(XATSHOME)/xassets/PY/xats2py
XATS2PY_PYEMIT01=\
$(XATSHOME)/xassets/JS/xats2py/xats2py_pyemit01_dats_opt1.js
#
########################################################################
########################################################################
pstrm000:: ; \
$(PRINTF) "## %s\n" "`date`" > OUTS/$@_out.py
pstrm000:: ; \
$(CAT) $(XATS2PYD)/runtime/xats2py_py1emit.py >> OUTS/$@_out.py
pstrm000:: ; \
$(CAT) $(XATS2PYD)/runtime/xats2py_prelude.py >> OUTS/$@_out.py
pstrm000:: pstrm000.dats; \
$(NODE) --stack-size=8192 $(XATS2PY_PYEMIT01) $@.dats >> OUTS/$@_out.py
########################################################################
fpath000:: ; \
$(PRINTF) "## %s\n" "`date`" > OUTS/$@_out.py
fpath000:: ; \
$(CAT) $(XATS2PYD)/runtime/xats2py_py1emit.py >> OUTS/$@_out.py
fpath000:: ; \
$(CAT) $(XATS2PYD)/runtime/xats2py_prelude.py >> OUTS/$@_out.py
fpath000:: ; \
$(CAT) $(XATSHOME)/contrib/githwxi/xatslib/DATS/CATS/PY/mylib00.cats >> OUTS/$@_out.py
fpath000:: fpath000.dats; \
$(NODE) --stack-size=8192 $(XATS2PY_PYEMIT01) $@.dats >> OUTS/$@_out.py
########################################################################
########################################################################
##################### end of [Makefile_pyemit01] #######################
