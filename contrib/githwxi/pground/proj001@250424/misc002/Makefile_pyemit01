########################################################################
CAT=cat
BUN=bun
ECHO=echo
NODE=node
PRINTF=printf
PYTHON=python3
########################################################################
########################################################################
all:: pstrm000_out.py
########################################################################
########################################################################
testall:: \
pstrm000_out.py; $(PYTHON) $<
########################################################################
########################################################################
clean:: ; rm -f *~
cleanall:: ; rm -f *~
cleanall:: ; rm -f pstrm000_out.py
########################################################################
########################################################################
XATSHOME=./../../../../../../XATSHOME
########################################################################
#
XATS2PY=$(XATSHOME)/xassets/PY/xats2py
#
XATS2PY_PYEMIT01=\
$(XATSHOME)/xassets/JS/xats2py/xats2py_pyemit01_dats_opt1.js
#
########################################################################
########################################################################
pstrm000_out.py:: ; \
$(PRINTF) "## %s\n" "`date`" > $@
pstrm000_out.py:: ; \
$(CAT) $(XATS2PY)/runtime/xats2py_py1emit.py >> $@
pstrm000_out.py:: ; \
$(CAT) $(XATS2PY)/runtime/xats2py_prelude.py >> $@
pstrm000_out.py:: ; \
$(NODE) --stack-size=8192 $(XATS2PY_PYEMIT01) pstrm000.dats >> $@
