########################################################################
########################################################################
# Makefile for misc002
########################################################################
########################################################################
CAT=cat
CPF=cp -f
ECHO=echo
NODE=node
PRINTF=printf
PYTHON=python3
########################################################################
########################################################################
all:: counter
########################################################################
clean:: ; rm -f *~
########################################################################
cleanall:: ; rm -f *~
cleanall:: ; rm -f OUTS/*_cats.js
cleanall:: ; rm -f OUTS/*_dats.js
########################################################################
XATSHOME=./../../../../../../XATSHOME
########################################################################
#
XATS2JSD=$(XATSHOME)/xassets/JS/xats2js
#
XATS2JS_JSEMIT01=\
$(XATSHOME)/xassets/JS/xats2js/xats2js_jsemit01_ats2_opt1.js
XATS2JS_JSEMIT01=\
$(XATSHOME)/xassets/JS/xats2js/xats2js_jsemit01_ats3_opt1.js
#
########################################################################
########################################################################
counter:: ; \
$(CPF) counter.cats OUTS/counter_cats.js
counter:: ; \
$(PRINTF) "// %s\n" "`date`" > OUTS/$@_dats.js
counter:: ; \
$(CAT) $(XATS2JSD)/runtime/xats2js_js1emit.js >> OUTS/$@_dats.js
counter:: ; \
$(CAT) $(XATS2JSD)/runtime/srcgen2_prelude.js >> OUTS/$@_dats.js
counter:: counter.dats; \
$(NODE) --stack-size=8192 $(XATS2JS_JSEMIT01) $< >> OUTS/$@_dats.js
########################################################################
########################################################################
##################### end of [Makefile_jsemit01] #######################
