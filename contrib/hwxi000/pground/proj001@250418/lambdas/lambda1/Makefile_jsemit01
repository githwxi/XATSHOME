########################################################################
########################################################################
# Makefile for
# CS525-2024F-lambdas-lambda1
########################################################################
########################################################################
CAT=cat
BUN=bun
ECHO=echo
NODE=node
PRINTF=printf
########################################################################
########################################################################
all:: lambda1_out.js
########################################################################
########################################################################
testall:: \
lambda1_out.js; $(NODE) $<
########################################################################
########################################################################
clean:: ; rm -f *~
cleanall:: ; rm -f *~
cleanall:: ; rm -f lambda1_out.js
cleanall:: ; rm -f lambda1_out?.js
########################################################################
########################################################################
XATSHOME=./../../../../../../../XATSHOME
########################################################################
#
XATS2JS=$(XATSHOME)/xassets/JS/xats2js
#
XATS2JS_JSEMIT01=\
$(XATSHOME)/xassets/JS/xats2js/xats2js_jsemit01_ats2_opt1.js
XATS2JS_JSEMIT01=\
$(XATSHOME)/xassets/JS/xats2js/xats2js_jsemit01_ats3_opt1.js
#
########################################################################
########################################################################
lambda1_out.js:: ; \
$(PRINTF) "// %s\n" "`date`" > $@
lambda1_out.js:: ; $(CAT) \
$(XATS2JS)/runtime/xats2js_js1emit.js >> $@
lambda1_out.js:: ; $(CAT) \
$(XATS2JS)/runtime/xats2js_prelude.js >> $@
# HX-2025-04-28:
# the flag '--_XATS2JS_' can soon be dropped!
lambda1_out.js:: ; $(NODE) \
--stack-size=8192 $(XATS2JS_JSEMIT01) lambda1.dats --_XATS2JS_ >> $@
########################################################################
lambda1_out1.js:: ; \
npx google-closure-compiler -W QUIET \
  --compilation_level SIMPLE --js=lambda1_out.js --js_output_file=$@
lambda1_out2.js:: ; \
npx google-closure-compiler -W QUIET \
  --compilation_level ADVANCED --js=lambda1_out.js --js_output_file=$@
########################################################################
########################################################################
##################### end of [Makefile_jsemit01] #######################
