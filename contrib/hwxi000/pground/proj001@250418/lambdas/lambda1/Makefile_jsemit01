########################################################################
########################################################################
# Makefile for
# CS525-2024F-lambdas-lambda1
########################################################################
########################################################################
CAT=cat
BUN=bun
ECHO=echo
NODE=node
PRINTF=printf
########################################################################
########################################################################
all:: lambda1
########################################################################
########################################################################
testall:: \
lambda1; $(NODE) $<_out.js
########################################################################
########################################################################
clean:: ; rm -f *~
cleanall:: ; rm -f *~
cleanall:: ; rm -f lambda1_out.js
cleanall:: ; rm -f lambda1_out?.js
########################################################################
########################################################################
XATSHOME=./../../../../../../../XATSHOME
########################################################################
#
XATS2JS=\
$(XATSHOME)/xassets/JS/xats2js
XATS2JS_JSEMIT01=\
$(XATS2JS)/xats2js_jsemit01_dats_opt0.js
#
########################################################################
########################################################################
lambda1:: ; \
$(PRINTF) "// %s\n" "`date`" > $@_out.js
lambda1:: ; \
$(CAT) $(XATS2JS)/runtime/xats2js_js1emit.js >> $@_out.js
lambda1:: ; \
$(CAT) $(XATS2JS)/runtime/xats2js_prelude.js >> $@_out.js
lambda1:: lambda1.dats; \
$(NODE) --stack-size=8192 $(XATS2JS_JSEMIT01) $< >> $@_out.js
########################################################################
lambda1_out1:: ; \
npx google-closure-compiler -W QUIET \
  --compilation_level SIMPLE --js=lambda1_out.js --js_output_file=lambda1_out1.js
lambda1_out2:: ; \
npx google-closure-compiler -W QUIET \
  --compilation_level ADVANCED --js=lambda1_out.js --js_output_file=lambda1_out2.js
########################################################################
########################################################################
##################### end of [Makefile_jsemit01] #######################
