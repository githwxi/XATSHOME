########################################################################
########################################################################
# Makefile for
# CS525-2024F-lambdas-lambda1
########################################################################
########################################################################
CAT=cat
BUN=bun
ECHO=echo
NODE=node
PRINTF=printf
PYTHON=python3
########################################################################
########################################################################
all:: lambda1_out.py
########################################################################
########################################################################
testall:: \
lambda1_out.py; $(PYTHON) $<
########################################################################
########################################################################
clean:: ; rm -f *~
cleanall:: ; rm -f *~
cleanall:: ; rm -f lambda1_out.py
########################################################################
########################################################################
XATSHOME=./../../../../../../../XATSHOME
########################################################################
#
XATS2JS=$(XATSHOME)/xassets/JS/xats2js
XATS2PY=$(XATSHOME)/xassets/PY/xats2py
#
XATS2PY_PYEMIT01=\
$(XATSHOME)/xassets/JS/xats2py/xats2py_pyemit01_ats2_opt1.js
# XATS2PY_PYEMIT01=\
# $(XATSHOME)/xassets/JS/xats2py/xats2py_pyemit01_ats3_opt1.js
#
########################################################################
########################################################################
lambda1_out.py:: ; \
$(PRINTF) "## %s\n" "`date`" > $@
lambda1_out.py:: ; $(CAT) \
$(XATS2PY)/runtime/xats2py_py1emit.py >> $@
lambda1_out.py:: ; $(CAT) \
$(XATS2PY)/runtime/xats2py_prelude.py >> $@
# HX-2025-04-28:
# the flag '--_XATS2PY_' can soon be dropped!
lambda1_out.py:: ; $(NODE) \
--stack-size=8192 $(XATS2PY_PYEMIT01) lambda1.dats --_XATS2PY_ >> $@
